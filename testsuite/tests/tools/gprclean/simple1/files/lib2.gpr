library project Lib2 is
   for Languages use ("Ada");
   for Source_Dirs use ("src2/**");
   for Object_Dir use "obj2";
   for Library_Name use project'Name;
   for Library_Dir use "lib2";
   for Library_Ali_Dir use project'Library_Dir & "-ali";
   for Externally_Built use "False";
   for Library_Kind use "dynamic";
   for Library_Src_Dir use project'Library_Dir & "-src";
   for Library_Auto_Init use "True";
   for Library_Interface use ("lib2", "lib2a", "gen2");
   for Library_Version use "liblib2.so.1.2";
end Lib2;
