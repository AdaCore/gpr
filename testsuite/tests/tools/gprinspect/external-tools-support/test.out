['gprinspect', '-Ptest', '--packages', '--attributes']
    - Packages          :
       - Check
          - Check'Default_Switches [ LIST ]
             - Index value : "Ada"
             - Values :
                - "--rules"
                - "my_rules.txt"
          - Check'Switches [ LIST ]
             - Index value : "test.adb"
             - Values :
                - ""
       - Codepeer
          - Codepeer'File_Patterns [ LIST ]
             - Values :
                - "*.ad?"
       - Prove
          - Prove'Switches [ LIST ]
             - Values :
                - "--report=all"
          - Prove'Proof_Switches [ LIST ]
             - Index value : "file.adb"
             - Values :
                - "--timeout=10"
          - Prove'Proof_Dir [ SINGLE ]
             - Value : "proof_dir"
       - Prove_Test
       - Check_Test
       - Test

