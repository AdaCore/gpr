main.adb
  single-unit          = TRUE
  has naming exception = FALSE
  - compilation unit at 1
    unit name    = Main
    kind         = S_BODY_ONLY
    withed units = { U Ada Pkg }
    object file  = main.o
    deps file    = main.ali
pkg.adb
  single-unit          = TRUE
  has naming exception = FALSE
  - compilation unit at 1
    unit name    = Pkg
    kind         = S_BODY
    object file  = pkg.o
    deps file    = pkg.ali
pkg.ads
  single-unit          = TRUE
  has naming exception = FALSE
  - compilation unit at 1
    unit name    = Pkg
    kind         = S_SPEC
    object file  = pkg.o
    deps file    = pkg.ali
sep.adb
  single-unit          = FALSE
  has naming exception = TRUE
  - compilation unit at 1
    unit name    = U.V
    kind         = S_SEPARATE
    withed units = { Ada Ada.Text_IO }
  - compilation unit at 2
    unit name    = U.W
    kind         = S_SEPARATE
    withed units = { GNAT GNAT.Regexp }
u.adb
  single-unit          = FALSE
  has naming exception = TRUE
  - compilation unit at 1
    unit name    = U
    kind         = S_SPEC
  - compilation unit at 2
    unit name    = U
    kind         = S_BODY
    withed units = { GNAT GNAT.OS_Lib }
    object file  = u~2.o
    deps file    = u~2.ali
