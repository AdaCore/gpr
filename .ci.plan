def build():
    anod_install("gprname")
    anod_build("gpr2", qualifier="legacy")
    anod_build("gprbuild", qualifier="gpr1")
    anod_build("gprbuild")

def bootstrap():
    anod_build("gnat")

def test():
    testgpr2()
    testgprbuild()
    testgpr2ls()
    anod_test("gpr2-bindings")

def testgpr2():
   anod_test("gpr2", qualifier="from_gnat")

def testgprbuild():
   anod_test("gprbuild")

def testgpr2ls():
   anod_build("gprbuild", qualifier="gpr1")
   anod_test("gprbuild", qualifier="check-gpr2ls")

def cov():
   anod_build("gpr2", qualifier="coverage")
   anod_test("gpr2", qualifier="coverage")
